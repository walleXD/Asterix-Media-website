@import 'materialize-css/dist/css/materialize.css'

#index
  & section
    margin: 0
    padding: 0

    /* height: 100vh */

    & h1
      margin: 0
      padding: 0

    & .row
      margin: 0

    & .section-title
      padding: 2%

    & .section-content
      padding: 2%

  & #landing

    & .logo-holder
      padding: 12% 0
    & .button-holder
      padding: 8% 0

  & #about

    & .content
      padding: 5% 0

      & p
        font-size: 110%
